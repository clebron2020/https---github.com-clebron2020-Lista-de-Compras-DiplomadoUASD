<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diplomadouasd.buylistapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.About"></activity>
        <activity android:name=".View.Products" />
        <activity android:name=".View.ProductCategory" />
        <activity android:name=".View.Settings" />
        <activity android:name=".View.SuperMarket" />
        <activity android:name=".View.CreateDetailBuyList" />
        <activity android:name=".View.ListDetail" />
        <activity android:name=".View.CreateBuyList" />
        <activity android:name=".View.ViewListCreated" />
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>